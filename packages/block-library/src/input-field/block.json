{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "core/input-field",
	"title": "Input field",
	"category": "common",
	"parent": [ "core/form" ],
	"description": "The basic building block for forms.",
	"keywords": [ "input", "text", "textarea" ],
	"textdomain": "default",
	"attributes": {
		"type": {
			"type": "string",
			"default": "text"
		},
		"label": {
			"type": "string",
			"default": "Label"
		},
		"inlineLabel": {
			"type": "boolean",
			"default": false
		}
	},
	"style": [ "wp-block-input-field" ]
}
